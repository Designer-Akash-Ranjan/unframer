{
    "name": "unframer",
    "version": "2.7.11",
    "description": "Import Framer components directly in your React app, type safe and customizable",
    "sideEffects": false,
    "repository": "https://github.com/remorses/unframer",
    "scripts": {
        "build": "rm -rf dist && pnpm concurrently 'pnpm tsc -m esnext --outDir esm' 'pnpm tsc' && echo copying && cp ../README.md ./README.md && cp ./src/framer.d.ts ./dist/framer.d.ts && cp ./src/framer.d.ts ./esm/framer.d.ts",
        "test": "vitest",
        "prepublishOnly": "pnpm build",
        "download-framer": "tsx scripts/download.ts",
        "watch": "rm -rf dist esm && pnpm concurrently 'pnpm tsc -w' 'pnpm tsc -m esnext -w --outDir esm'"
    },
    "bin": "bin.js",
    "files": [
        "dist",
        "framer-fixed",
        "bin.js",
        "src",
        "README.md",
        "esm"
    ],
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "module": "./esm/index.js",
            "default": "./dist/index.js"
        },
        "./src/framer.js": {
            "types": "./dist/framer.d.ts",
            "default": "./src/framer.js"
        },
        "./dist/exporter": {
            "types": "./dist/exporter.d.ts",
            "module": "./esm/exporter.js",
            "default": "./dist/exporter.js"
        },
        "./package.json": "./package.json"
    },
    "keywords": [],
    "author": "Tommaso De Rossi, morse <beats.by.morse@gmail.com>",
    "license": "",
    "peerDependencies": {
        "react": "*",
        "react-dom": "*"
    },
    "dependencies": {
        "async-sema": "^3.1.1",
        "cac": "^6.7.14",
        "dprint-node": "^1.0.8",
        "esbuild": "^0.24.0",
        "esbuild-plugins-node-modules-polyfill": "^1.6.8",
        "eventemitter3": "^3.1.2",
        "find-up": "^5.0.0",
        "fs-extra": "^11.2.0",
        "json5": "^2.2.3",
        "just-kebab-case": "^4.2.0",
        "marked": "^15.0.3",
        "marked-terminal": "^7.2.1",
        "nanospinner": "^1.2.0",
        "native-fetch": "^4.0.2",
        "picocolors": "^1.0.1",
        "real-framer-motion": "npm:framer-motion@11.2.10",
        "string-dedent": "^3.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.24.9",
        "@babel/helper-annotate-as-pure": "^7.24.7",
        "@babel/helper-environment-visitor": "^7.24.7",
        "@babel/helper-split-export-declaration": "^7.24.7",
        "@babel/plugin-transform-react-pure-annotations": "^7.24.7",
        "@babel/traverse": "^7.24.8",
        "@babel/types": "^7.24.9",
        "@types/babel__core": "^7.20.5",
        "@types/babel__traverse": "^7.20.6",
        "@types/bun": "^1.1.6",
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^22.10.2",
        "@types/react": "^18.3.12",
        "@xmorse/deployment-utils": "^0.2.19",
        "concurrently": "^8.2.2",
        "openai": "^4.52.7",
        "posthtml": "^0.16.6",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "tiktoken": "^1.0.15",
        "typescript": "^5.6.2"
    }
}
