{
    "name": "unframer",
    "version": "2.2.1",
    "description": "",
    "repository": "https://github.com/remorses/unframer",
    "scripts": {
        "build": "rm -rf dist && tsc -m esnext --outDir esm && tsc && cp ../README.md ./README.md && cp ./src/framer.d.ts ./dist/framer.d.ts && cp ./src/framer.d.ts ./esm/framer.d.ts",
        "prepublishOnly": "pnpm build",
        "download-framer": "tsx scripts/download.ts",
        "watch": "rm -rf dist && tsc -w"
    },
    "bin": "bin.js",
    "files": [
        "dist",
        "framer-fixed",
        "bin.js",
        "src",
        "README.md",
        "esm"
    ],
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "module": "./esm/index.js",
            "default": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "keywords": [],
    "author": "Tommaso De Rossi, morse <beats.by.morse@gmail.com>",
    "license": "",
    "peerDependencies": {
        "@types/react": "*",
        "react": "*",
        "react-dom": "*"
    },
    "dependencies": {
        "async-sema": "^3.1.1",
        "cac": "^6.7.14",
        "chokidar": "^3.5.3",
        "dedent": "^1.5.1",
        "dprint-node": "^1.0.8",
        "esbuild": "^0.20.2",
        "esbuild-plugin-polyfill-node": "^0.3.0",
        "eventemitter3": "^3.1.0",
        "find-up": "^5.0.0",
        "fs-extra": "^11.1.1",
        "json5": "^2.2.3",
        "native-fetch": "^4.0.2",
        "picocolors": "^1.0.0",
        "real-framer-motion": "npm:framer-motion@11.0.14",
        "tmp": "^0.2.1"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^20.5.7",
        "@xmorse/deployment-utils": "^0.1.0",
        "posthtml": "^0.16.6",
        "react": "^18.3.0",
        "react-dom": "^18.3.0"
    }
}
