{
    "name": "unframer",
    "version": "2.5.3",
    "description": "Import Framer components directly in your React app, type safe and customizable",
    "sideEffects": false,
    "repository": "https://github.com/remorses/unframer",
    "scripts": {
        "build": "rm -rf dist && tsc -m esnext --outDir esm && tsc && cp ../README.md ./README.md && cp ./src/framer.d.ts ./dist/framer.d.ts && cp ./src/framer.d.ts ./esm/framer.d.ts",
        "prepublishOnly": "pnpm build",
        "download-framer": "tsx scripts/download.ts",
        "watch": "rm -rf dist esm && concurrently 'tsc -w' 'tsc -m esnext -w --outDir esm'"
    },
    "bin": "bin.js",
    "files": [
        "dist",
        "framer-fixed",
        "bin.js",
        "src",
        "README.md",
        "esm"
    ],
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "module": "./esm/index.js",
            "default": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "keywords": [],
    "author": "Tommaso De Rossi, morse <beats.by.morse@gmail.com>",
    "license": "",
    "peerDependencies": {
        "react": "*",
        "react-dom": "*"
    },
    "dependencies": {
        "async-sema": "^3.1.1",
        "cac": "^6.7.14",
        "chokidar": "^3.5.3",
        "dedent": "^1.5.1",
        "dprint-node": "^1.0.8",
        "esbuild": "^0.21.4",
        "esbuild-plugin-polyfill-node": "^0.3.0",
        "eventemitter3": "^3.1.0",
        "find-up": "^5.0.0",
        "fs-extra": "^11.1.1",
        "json5": "^2.2.3",
        "native-fetch": "^4.0.2",
        "picocolors": "^1.0.0",
        "real-framer-motion": "npm:framer-motion@11.2.10",
        "tmp": "^0.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.24.5",
        "@babel/helper-annotate-as-pure": "^7.22.5",
        "@babel/helper-environment-visitor": "^7.22.20",
        "@babel/helper-split-export-declaration": "^7.24.5",
        "@babel/plugin-transform-react-pure-annotations": "^7.24.1",
        "@babel/traverse": "^7.24.5",
        "@babel/types": "^7.24.5",
        "@types/babel__core": "^7.20.5",
        "@types/babel__traverse": "^7.20.5",
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^20.5.7",
        "@xmorse/deployment-utils": "^0.1.0",
        "concurrently": "^8.2.2",
        "openai": "^4.40.2",
        "posthtml": "^0.16.6",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "tiktoken": "^1.0.14"
    }
}
