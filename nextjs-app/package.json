    "dependencies": {
        "@types/node": "22.7.4",
        "@types/react": "18.3.5",
        "@types/react-dom": "18.3.0",
        "autoprefixer": "10.4.14",
        "concurrently-ui": "^1.2.0",
        "next": "15.1.6",
        "postcss": "8.4.24",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "tailwindcss": "3.3.2",
        "unframer": "workspace:*"
    }
